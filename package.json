{
  "name": "root",
  "private": true,
  "scripts": {
    "build": "pnpm --filter moda-sdk build",
    "dev": "pnpm --filter moda-sdk watch",
    "c:install": "pnpm --filter moda-contracts install",
    "c:deploy_Registry": "pnpm --filter moda-contracts deploy_Registry",
    "c:deploy_Profile": "pnpm --filter moda-contracts deploy_Profile",
    "c:deploy_Management": "pnpm --filter moda-contracts deploy_Management",
    "c:deploy_SplitsFactory": "pnpm --filter moda-contracts deploy_SplitsFactory",
    "c:deploy_CatalogFactory": "pnpm --filter moda-contracts deploy_CatalogFactory",
    "c:set_OfficialContracts": "pnpm --filter moda-contracts set_OfficialContracts",
    "c:docs": "pnpm --filter moda-contracts contracts:docs",
    "format": "biome format . --write",
    "lint": "biome check .",
    "lint:fix": "pnpm lint --apply-unsafe",
    "lint:ci": "biome ci .",
    "update": "pnpm update --latest --recursive --interactive",
    "generate:profile": "pnpm --filter profile run build",
    "evm-profile:dev": "pnpm --filter evm-profile run dev"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3"
  },
  "packageManager": "pnpm@8.10.1"
}
